Include config.d/*.conf

Host *
  AddKeysToAgent yes
  Compression yes
  ExitOnForwardFailure yes
  ForwardAgent no
  ForwardX11 no
  GSSAPIAuthentication no
  HashKnownHosts no
  IdentityFile ~/.ssh/keys/%h
  PasswordAuthentication no
  PubkeyAuthentication yes
  ServerAliveCountMax 8
  ServerAliveInterval 15
  TCPKeepAlive yes
