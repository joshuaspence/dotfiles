" Plugins "{{{
    source ~/.vim/bundles.vim
" "}}}

" Regular vim configuration "{{{
    source ~/.vim/config.vim
" "}}}

" Key mappings "{{{
    source ~/.vim/mappings.vim
" "}}}

" Custom functions "{{{
    source ~/.vim/functions.vim
" "}}}

" Commands "{{{
    source ~/.vim/commands.vim
" "}}}

" Auto commands "{{{
    if has("autocmd")
        source ~/.vim/autocmd.vim
    endif
" "}}}
