# Unset environment variables.
unset MANPATH

# We need the path functions!
source "$HOME/.shell/functions/path"

# ~/bin should be in the front of PATH if it exists.
if [ -d "$HOME/bin" ]; then
    prepend-path PATH "$HOME/bin"
fi

# ~/man should be in the front of MANPATH if it exists.
if [ -d "$HOME/man" ]; then
    MANPATH="$(manpath)"
    prepend-path MANPATH "$HOME/man"
fi

# Export environment variables.
export PATH
export MANPATH
