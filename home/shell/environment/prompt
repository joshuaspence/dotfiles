if ! command -v shell_prompt >/dev/null; then
    command -v sh_source_r >/dev/null || source "$HOME/.shell/functions/source/sh_source_r"
    sh_source_r "$HOME/.shell/functions/prompt"
fi

# Check for color support.
[[ -n $HAS_COLOR ]] || source "$HOME/.shell/environment/color"

export PS1="$(shell_title)$(shell_prompt)"
export PS2='... > '
export PS3='#? '
export PS4='+'
